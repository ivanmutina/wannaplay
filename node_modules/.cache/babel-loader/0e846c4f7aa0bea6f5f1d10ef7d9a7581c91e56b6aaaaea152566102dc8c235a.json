{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport API from \"../api\";\nexport default {\n  data() {\n    return {\n      post: {\n        title: \"\",\n        date: \"\",\n        time: \"\",\n        place: \"\",\n        gender: \"\",\n        contact: \"\",\n        description: \"\"\n      }\n    };\n  },\n  async created() {\n    const response = await API.getPostByID(this.$route.params.id);\n    this.post = response;\n  },\n  methods: {\n    async submitForm() {\n      const formData = new FormData();\n      formData.append(\"title\", this.post.title);\n      formData.append(\"date\", this.post.date);\n      formData.append(\"time\", this.post.time);\n      formData.append(\"place\", this.post.place);\n      formData.append(\"gender\", this.post.gender);\n      formData.append(\"contact\", this.post.contact);\n      formData.append(\"description\", this.post.description);\n\n      // ako sve valja\n      try {\n        const response = await API.addPost(formData);\n        // prebaci odma na home\n        this.$router.push({\n          name: \"home\",\n          params: {\n            message: response.message\n          }\n        });\n      } catch (error) {\n        console.error(\"Error occurred while sending the request:\", error);\n      }\n    }\n  },\n  mounted() {\n    // refresham stranicu kada se otvori\n    if (!sessionStorage.getItem(\"isPageRefreshed\")) {\n      // osvježi stranicu samo ako nije već osvježena\n      sessionStorage.setItem(\"isPageRefreshed\", \"true\");\n      window.location.reload();\n    } else {\n      sessionStorage.removeItem(\"isPageRefreshed\");\n    }\n  }\n};","map":{"version":3,"names":["API","data","post","title","date","time","place","gender","contact","description","created","response","getPostByID","$route","params","id","methods","submitForm","formData","FormData","append","addPost","$router","push","name","message","error","console","mounted","sessionStorage","getItem","setItem","window","location","reload","removeItem"],"sources":["C:\\Users\\imutina\\Projects\\wannaplay\\src\\views\\EditEvent.vue"],"sourcesContent":["<template>\r\n  <div style=\"height: 100vh; overflow-y: auto\">\r\n    <div class=\"container mt-5\">\r\n      <!-- First part -->\r\n      <div class=\"row mb-4 pb-2\">\r\n        <div class=\"col-md-8\">\r\n          <div class=\"text-md-start\">\r\n            <h3 class=\"title mb-3 desc\">Edit event</h3>\r\n\r\n            <p class=\"mb-0 mt-4 desc\">Please edit all the necessary information to make the post more effective.</p>\r\n            <p class=\"desc\">Good luck with finding players!</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- /First part -->\r\n\r\n      <!-- Form -->\r\n      <div>\r\n        <div class=\"row justify-content-center\">\r\n          <div class=\"col-12 text-center align-self-center\">\r\n            <div class=\"section pb-5 text-center mb-5\">\r\n              <!--  -->\r\n              <form @submit.prevent=\"updateForm\" ref=\"form\">\r\n                <div class=\"card-wrap mx-auto mb-5\">\r\n                  <div class=\"card-wrapper mb-5\">\r\n                    <!-- inputs -->\r\n                    <div class=\"card\">\r\n                      <div class=\"center-wrap\">\r\n                        <div class=\"section text-center\">\r\n                          <div class=\"form-group\">\r\n                            <input type=\"text\" v-model=\"post.title\" class=\"form-style\" placeholder=\"Title\" autocomplete=\"off\" required />\r\n                            <i class=\"input-icon fas fa-pencil-alt\"></i>\r\n                          </div>\r\n                          <div class=\"form-group mt-2\">\r\n                            <input type=\"text\" v-model=\"post.date\" class=\"form-style\" placeholder=\"Date\" autocomplete=\"off\" required />\r\n                            <i class=\"input-icon fas fa-calendar-alt\"></i>\r\n                          </div>\r\n                          <div class=\"form-group mt-2\">\r\n                            <input type=\"text\" v-model=\"post.time\" class=\"form-style\" placeholder=\"Time\" autocomplete=\"off\" required />\r\n                            <i class=\"input-icon fas fa-clock\"></i>\r\n                          </div>\r\n                          <div class=\"form-group mt-2\">\r\n                            <input type=\"text\" name=\"place\" label=\"place\" v-model=\"post.place\" class=\"form-style\" placeholder=\"PLace\" autocomplete=\"off\" required />\r\n                            <i class=\"input-icon fas fa-map-marker-alt\"></i>\r\n                          </div>\r\n                          <div class=\"form-group mt-2\">\r\n                            <input type=\"text\" v-model=\"post.gender\" class=\"form-style\" placeholder=\"Gender\" autocomplete=\"off\" required />\r\n                            <i class=\"input-icon fas fa-venus-mars\"></i>\r\n                          </div>\r\n                          <div class=\"form-group mt-2\">\r\n                            <input type=\"text\" v-model=\"post.contact\" class=\"form-style\" placeholder=\"Contact (number)\" autocomplete=\"off\" required />\r\n                            <i class=\"input-icon fas fa-phone\"></i>\r\n                          </div>\r\n                          <div class=\"form-group mt-2\">\r\n                            <textarea v-model=\"post.description\" name=\"description\" label=\"description\" class=\"form-style\" placeholder=\"Description\" autocomplete=\"off\" rows=\"4\"></textarea>\r\n                            <i class=\"input-icon fas fa-info-circle\"></i>\r\n                            <small>All information will be publicly disclosed.</small>\r\n                          </div>\r\n\r\n                          <button type=\"submit\" class=\"btn mt-4\">Update</button>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- /Form -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      post: {\r\n        title: \"\",\r\n        date: \"\",\r\n        time: \"\",\r\n        place: \"\",\r\n        gender: \"\",\r\n        contact: \"\",\r\n        description: \"\",\r\n      },\r\n    };\r\n  },\r\n  async created() {\r\n    const response = await API.getPostByID(this.$route.params.id);\r\n    this.post = response;\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      const formData = new FormData();\r\n      formData.append(\"title\", this.post.title);\r\n      formData.append(\"date\", this.post.date);\r\n      formData.append(\"time\", this.post.time);\r\n      formData.append(\"place\", this.post.place);\r\n      formData.append(\"gender\", this.post.gender);\r\n      formData.append(\"contact\", this.post.contact);\r\n      formData.append(\"description\", this.post.description);\r\n\r\n      // ako sve valja\r\n      try {\r\n        const response = await API.addPost(formData);\r\n        // prebaci odma na home\r\n        this.$router.push({ name: \"home\", params: { message: response.message } });\r\n      } catch (error) {\r\n        console.error(\"Error occurred while sending the request:\", error);\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    // refresham stranicu kada se otvori\r\n    if (!sessionStorage.getItem(\"isPageRefreshed\")) {\r\n      // osvježi stranicu samo ako nije već osvježena\r\n      sessionStorage.setItem(\"isPageRefreshed\", \"true\");\r\n      window.location.reload();\r\n    } else {\r\n      sessionStorage.removeItem(\"isPageRefreshed\");\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@import url(\"https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800,900\");\r\n\r\nbody {\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-weight: 300;\r\n  font-size: 15px;\r\n  line-height: 1.7;\r\n  background-color: #1f2029;\r\n  overflow: hidden;\r\n}\r\n\r\np {\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-weight: 400;\r\n  font-size: 16px;\r\n  line-height: 1.7;\r\n}\r\n\r\n/* prvi tekst */\r\n.desc {\r\n  color: rgb(255, 254, 254);\r\n}\r\n\r\n/* forma */\r\na {\r\n  cursor: pointer;\r\n  transition: all 200ms linear;\r\n}\r\na:hover {\r\n  text-decoration: none;\r\n}\r\n.link {\r\n  color: #c4c3ca;\r\n}\r\n.link:hover {\r\n  color: #ffeba7;\r\n}\r\n\r\n.card-wrap {\r\n  width: 700px;\r\n  max-width: 100%;\r\n  height: 600px;\r\n  perspective: 800px;\r\n  margin-bottom: 60px;\r\n}\r\n.card-wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n.card {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: #2a2b38;\r\n  border-radius: 6px;\r\n  left: 0;\r\n  top: 0;\r\n}\r\n\r\n.center-wrap {\r\n  position: absolute;\r\n  width: 100%;\r\n  padding: 0 35px;\r\n  top: 50%;\r\n  left: 0;\r\n  transform: translate3d(0, -50%, 35px) perspective(100px);\r\n}\r\n\r\n.form-group {\r\n  position: relative;\r\n  display: block;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.form-style {\r\n  padding: 13px 20px;\r\n  padding-left: 55px;\r\n  height: 48px;\r\n  width: 100%;\r\n  font-weight: 500;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  line-height: 22px;\r\n  letter-spacing: 0.5px;\r\n  outline: none;\r\n  color: #c4c3ca;\r\n  background-color: #1f2029;\r\n  border: none;\r\n  box-shadow: 0 4px 8px 0 rgba(21, 21, 21, 0.2);\r\n}\r\n\r\n.input-icon {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 17px;\r\n  font-size: 24px;\r\n  line-height: 48px;\r\n  text-align: left;\r\n  color: #ffeba7;\r\n}\r\n\r\n.btn {\r\n  border-radius: 5px;\r\n  height: 44px;\r\n  font-size: 15px;\r\n  font-weight: 600;\r\n  padding: 0 30px;\r\n  letter-spacing: 1px;\r\n  display: -webkit-inline-flex;\r\n  display: -ms-inline-flexbox;\r\n  display: inline-flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border: none;\r\n  background-color: #ffeba7;\r\n  color: #102770;\r\n  box-shadow: 0 8px 24px 0 rgba(255, 235, 167, 0.2);\r\n}\r\n.btn:active,\r\n.btn:focus {\r\n  background-color: #102770;\r\n  color: #ffeba7;\r\n  box-shadow: 0 8px 24px 0 rgba(16, 39, 112, 0.2);\r\n}\r\n.btn:hover {\r\n  background-color: #102770;\r\n  color: #ffeba7;\r\n  box-shadow: 0 8px 24px 0 rgba(16, 39, 112, 0.2);\r\n}\r\n\r\ntextarea.form-style {\r\n  height: 100px;\r\n  max-height: 140px;\r\n}\r\nsmall {\r\n  font-size: 13px;\r\n  color: #888;\r\n}\r\n</style>\r\n"],"mappings":";AA4EA,OAAOA,GAAE,MAAO,QAAQ;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE,EAAE;QACVC,OAAO,EAAE,EAAE;QACXC,WAAW,EAAE;MACf;IACF,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,QAAO,GAAI,MAAMX,GAAG,CAACY,WAAW,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;IAC7D,IAAI,CAACb,IAAG,GAAIS,QAAQ;EACtB,CAAC;EACDK,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAAClB,IAAI,CAACC,KAAK,CAAC;MACzCe,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAAClB,IAAI,CAACE,IAAI,CAAC;MACvCc,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAAClB,IAAI,CAACG,IAAI,CAAC;MACvCa,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAAClB,IAAI,CAACI,KAAK,CAAC;MACzCY,QAAQ,CAACE,MAAM,CAAC,QAAQ,EAAE,IAAI,CAAClB,IAAI,CAACK,MAAM,CAAC;MAC3CW,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAAClB,IAAI,CAACM,OAAO,CAAC;MAC7CU,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAAClB,IAAI,CAACO,WAAW,CAAC;;MAErD;MACA,IAAI;QACF,MAAME,QAAO,GAAI,MAAMX,GAAG,CAACqB,OAAO,CAACH,QAAQ,CAAC;QAC5C;QACA,IAAI,CAACI,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE,MAAM;UAAEV,MAAM,EAAE;YAAEW,OAAO,EAAEd,QAAQ,CAACc;UAAQ;QAAE,CAAC,CAAC;MAC5E,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2CAA2C,EAAEA,KAAK,CAAC;MACnE;IACF;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,cAAc,C