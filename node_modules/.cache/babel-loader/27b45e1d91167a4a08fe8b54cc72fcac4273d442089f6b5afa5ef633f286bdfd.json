{"ast":null,"code":"import API from \"../api\";\nimport router from \"@/router\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      posts: [],\n      selectedPost: {}\n    };\n  },\n  // dohvati sve postove\n  async created() {\n    this.posts = await API.getAllPost();\n  },\n  methods: {\n    async modal(postId) {\n      try {\n        // dohvati post detalje preko API\n        const post = await API.getPostByID(postId);\n        // postavim 'selectedPost' na dobivene detalje posta\n        this.selectedPost = post;\n        // pokaž modal, jQuery\n        $(\"#exampleModal\").modal(\"show\");\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    formatDate(created) {\n      // formatiram created u dd.mm.yyyy\n      return new Date(created).toLocaleDateString(\"hr-HR\");\n    },\n    editPost(postId) {\n      // preusmjeri na stranicu edit-event s odgovarajućim ID\n      this.$router.replace({\n        name: \"edit-event\",\n        params: {\n          id: postId\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["API","router","name","data","posts","selectedPost","created","getAllPost","methods","modal","postId","post","getPostByID","$","error","console","formatDate","Date","toLocaleDateString","editPost","$router","replace","params","id"],"sources":["C:\\Users\\imutina\\Projects\\wannaplay\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"container mt-5\">\r\n      <!-- prvi dio -->\r\n      <div class=\"row mb-4 pb-2\">\r\n        <div class=\"col-md-8\">\r\n          <div class=\"text-md-start\">\r\n            <h3 class=\"title mb-3 desc\">Find the perfect match</h3>\r\n            <!-- search -->\r\n            <div class=\"col-3\">\r\n              <form class=\"form-inline mt-4\">\r\n                <input class=\"form-control mr-sm-2\" type=\"search\" placeholder=\"Search\" aria-label=\"Search\" />\r\n              </form>\r\n            </div>\r\n            <!-- /search -->\r\n            <p class=\"mb-0 mt-4 desc\">Do you want to play football, basketball, or any other sport but don't have anyone to play with?</p>\r\n            <p class=\"desc\">Or do you just need a few more players? Find a team and start playing TODAY!</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- /prvi dio -->\r\n\r\n      <!-- kartica -->\r\n      <div class=\"row\">\r\n        <div class=\"col-lg-4 col-md-6 col-12 mt-1 pt-2\" v-for=\"post in posts\" :key=\"post._id\">\r\n          <div class=\"card border-0 bg-light rounded shadow\">\r\n            <div class=\"card-body p-4\">\r\n              <span class=\"badge rounded-pill float-md-end mb-3 mb-sm-0\">{{ post.gender }}</span>\r\n              <h5>{{ post.title }}</h5>\r\n              <div class=\"mt-3\">\r\n                <span> {{ post.description.substring(0, 40) + \"...\" }}</span>\r\n                <span class=\"d-block mt-1\"><i class=\"far fa-calendar-alt\" aria-hidden=\"true\"></i> {{ post.date }}</span>\r\n                <span class=\"d-block\"><i class=\"fas fa-map-marker-alt\" aria-hidden=\"true\"></i> {{ post.place }}</span>\r\n              </div>\r\n              <!-- gumb -->\r\n              <div class=\"d-flex justify-content-between align-items-center mt-3\">\r\n                <button type=\"button\" class=\"btn\" data-bs-toggle=\"modal\" data-bs-target=\"#exampleModal\" @click=\"modal(post._id)\">View details</button>\r\n                <small class=\"created-text pt-4\">Created {{ formatDate(post.created) }}</small>\r\n              </div>\r\n\r\n              <!-- /gumb -->\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- /kartica -->\r\n      </div>\r\n    </div>\r\n\r\n    <!-- modal -->\r\n    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <!-- modal header -->\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\" id=\"exampleModalLabel\">{{ selectedPost.title }}</h5>\r\n            <span class=\"badge rounded-pill float-md-start me-3 ms-3 mb-3 mb-sm-0 text-start\">{{ selectedPost.gender }}</span>\r\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n          </div>\r\n          <!-- modal body -->\r\n          <div class=\"modal-body\">\r\n            <p class=\"text-start\"><i class=\"fas fa-info-circle\"></i> {{ selectedPost.description }}</p>\r\n            <p class=\"text-start\">\r\n              The event will take place on <span class=\"post-span\">{{ selectedPost.date }} </span> at <span class=\"post-span\">{{ selectedPost.time }}</span> in <span class=\"post-span\">{{ selectedPost.place }}.</span>\r\n            </p>\r\n\r\n            <p class=\"text-start\"><i class=\"fas fa-phone\"></i> +385 958128238</p>\r\n          </div>\r\n\r\n          <!-- modal footer -->\r\n          <div class=\"modal-footer\">\r\n            <button class=\"edit-btn\" @click=\"urediPost(selectedPost._id)\">Edit</button>\r\n            <button class=\"close-btn\">Delete</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- /modal -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../api\";\r\nimport router from \"@/router\";\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      posts: [],\r\n      selectedPost: {},\r\n    };\r\n  },\r\n  // dohvati sve postove\r\n  async created() {\r\n    this.posts = await API.getAllPost();\r\n  },\r\n  methods: {\r\n    async modal(postId) {\r\n      try {\r\n        // dohvati post detalje preko API\r\n        const post = await API.getPostByID(postId);\r\n        // postavim 'selectedPost' na dobivene detalje posta\r\n        this.selectedPost = post;\r\n        // pokaž modal, jQuery\r\n        $(\"#exampleModal\").modal(\"show\");\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    },\r\n    formatDate(created) {\r\n      // formatiram created u dd.mm.yyyy\r\n      return new Date(created).toLocaleDateString(\"hr-HR\");\r\n    },\r\n    editPost(postId) {\r\n      // preusmjeri na stranicu edit-event s odgovarajućim ID\r\n      this.$router.replace({ name: \"edit-event\", params: { id: postId } });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@import url(\"https://fonts.googleapis.com/css?family=Poppins:400,500,600,700,800,900\");\r\n\r\nbody {\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-weight: 300;\r\n  font-size: 15px;\r\n  line-height: 1.7;\r\n  background-color: #1f2029;\r\n  overflow: hidden;\r\n}\r\n.created-text {\r\n  display: inline-block;\r\n  margin-left: auto;\r\n  font-size: 12px;\r\n  color: #c4c3ca;\r\n}\r\n\r\np {\r\n  font-family: \"Poppins\", sans-serif;\r\n  font-weight: 400;\r\n  font-size: 16px;\r\n  line-height: 1.7;\r\n}\r\n\r\n/* prvi dio - opis */\r\n.desc {\r\n  color: rgb(255, 254, 254);\r\n}\r\n\r\n/* na kartici */\r\n.rounded-pill {\r\n  background-color: #102770;\r\n}\r\n\r\n/* gumb na kartici*/\r\n.card .btn {\r\n  position: relative;\r\n  font-weight: 550;\r\n  font-size: 15px;\r\n  line-height: 2;\r\n  height: 50px;\r\n  width: 150px;\r\n  white-space: nowrap;\r\n  transition: all 200ms linear;\r\n  border-radius: 5px;\r\n  letter-spacing: 1px;\r\n  cursor: pointer;\r\n  border: none;\r\n  background-color: #102770;\r\n  color: #ffeba7;\r\n  box-shadow: 0 12px 35px 0 rgba(16, 39, 112, 0.25);\r\n}\r\n.card .btn:active,\r\n.card .btn:focus {\r\n  background-color: #ffeba7;\r\n  color: #102770;\r\n  box-shadow: 0 8px 24px 0 rgba(16, 39, 112, 0.2);\r\n}\r\n.card .btn:hover {\r\n  background-color: #ffeba7;\r\n  color: #102770;\r\n  box-shadow: 0 8px 24px 0 rgba(16, 39, 112, 0.2);\r\n}\r\n\r\n/* modal */\r\n.close-btn {\r\n  font-weight: 420;\r\n  letter-spacing: 1.5px;\r\n  border: none;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  transition: all 200ms linear;\r\n  color: #ffeba7;\r\n  background-color: #832a45;\r\n  box-shadow: 0 8px 24px 0 rgba(16, 39, 112, 0.2);\r\n}\r\n\r\n.edit-btn {\r\n  font-weight: 420;\r\n  letter-spacing: 1.5px;\r\n  border: none;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  transition: all 200ms linear;\r\n  color: #ffeba7;\r\n  background-color: #5bbd9c;\r\n  box-shadow: 0 8px 24px 0 rgba(16, 39, 112, 0.2);\r\n}\r\n\r\n.edit-btn:hover,\r\n.close-btn:hover {\r\n  color: #102770;\r\n  background-color: rgb(249, 244, 244);\r\n}\r\n\r\n.post-span {\r\n  color: #102770;\r\n  font-weight: 300;\r\n}\r\n</style>\r\n"],"mappings":"AAiFA,OAAOA,GAAE,MAAO,QAAQ;AACxB,OAAOC,MAAK,MAAO,UAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,CAAC;IACjB,CAAC;EACH,CAAC;EACD;EACA,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI,CAACF,KAAI,GAAI,MAAMJ,GAAG,CAACO,UAAU,CAAC,CAAC;EACrC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAACC,MAAM,EAAE;MAClB,IAAI;QACF;QACA,MAAMC,IAAG,GAAI,MAAMX,GAAG,CAACY,WAAW,CAACF,MAAM,CAAC;QAC1C;QACA,IAAI,CAACL,YAAW,GAAIM,IAAI;QACxB;QACAE,CAAC,CAAC,eAAe,CAAC,CAACJ,KAAK,CAAC,MAAM,CAAC;MAClC,EAAE,OAAOK,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB;IACF,CAAC;IACDE,UAAUA,CAACV,OAAO,EAAE;MAClB;MACA,OAAO,IAAIW,IAAI,CAACX,OAAO,CAAC,CAACY,kBAAkB,CAAC,OAAO,CAAC;IACtD,CAAC;IACDC,QAAQA,CAACT,MAAM,EAAE;MACf;MACA,IAAI,CAACU,OAAO,CAACC,OAAO,CAAC;QAAEnB,IAAI,EAAE,YAAY;QAAEoB,MAAM,EAAE;UAAEC,EAAE,EAAEb;QAAO;MAAE,CAAC,CAAC;IACtE;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}